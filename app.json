{
  "expo": {
    "name": "health-habitz-app",
    "slug": "health-habitz-app",
    "version": "1.0.0",
    "scheme": "health-habitz-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-dynamic-app-icon",
        {
          "default": {
            "image": "./assets/icon.png",
            "prerendered": true
          },
          "dark": {
            "image": "./assets/icon-dark.png",
            "prerendered": true
          },
          "green": {
            "image": "./assets/icon-green.png",
            "prerendered": true
          },
          "blue": {
            "image": "./assets/icon-blue.png",
            "prerendered": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.healthhabitzapp",
      "appleTeamId": "6DK584RWB8"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.healthhabitzapp"
    }
  }
}